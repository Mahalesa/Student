@{
}
@model List<Student.Models.Entities.Student>

<h1>Students</h1>
<table class="table">
    <thead>
        <th>Id</th>
        <th>Name</th>
        <th>Email</th>
        <th> </th>

    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <th>@student.ID</th>
                <th>@student.Name</th>
                <th>@student.Email</th>
                <td> 
                    <a>Edit</a>
                </td>

            </tr>
        }
    </tbody>
</table>
